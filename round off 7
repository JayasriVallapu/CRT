#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float a;
    scanf("%f",&a);
    float c,b;
    c=floor(a);
    b=ceil(a);
    printf("%d\n",(int)a);
    printf("%.1f\n",b);
    printf("%.1f\n",c);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
