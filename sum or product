#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b;
    scanf("%d%d",&a,&b);
    int p;
    p=a*b;
        if(p>1000)
        {
            printf("%d",a+b);
        }
    else{
        printf("%d",a*b);
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
