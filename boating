int main() {
    int w,a,c;
    scanf("%d%d%d",&w,&a,&c);
  int r=a*75+c*30;
    if(r>w){
        printf("Boat will drown");
    }
        else{
            printf("Boat is stable");
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
